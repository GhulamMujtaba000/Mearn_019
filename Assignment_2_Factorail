<body>

    <script>
        let user=prompt("Please Enter the Factorial!");
        let result=1;

        for (let index = 1; index <= user; index++) {
            let num=0;
            num=result*index;
            result=num;
            console.log("Factorial  !"+result);

        }

    </script>
    
</body>
